{
  "parser"  : "babel-eslint",
  "extends" : [
    "airbnb"
  
  ],
  "plugins": [
    "react"
  ],
  "env"     : {
    "browser" : true,
    "node": true,
  },
  "globals" : {
    "__DEV__"      : false,
    "__PROD__"     : false,
    "__DEBUG__"    : false,
    "__COVERAGE__" : false,
    "__BASENAME__" : false
  },
  "rules": {
    "indent": [1, 4],
    "no-debugger": 0,
    "no-console": 0,
    "new-cap": 0,
    "strict": 0,
    "no-underscore-dangle": 0,
    "no-use-before-define": 0,
    "eol-last": 0,
    "quotes": [2, "single"],
    "jsx-quotes": [1, "prefer-single"],
    "no-else-return": 0,
	"import/export": 0,
	"object-shorthand": 0,
	"max-len": 0,
	"func-names":0,
	"prefer-arrow-callback": 0,
    "react/jsx-no-undef": 1,
    "react/jsx-uses-react": 1,
    "react/jsx-uses-vars": 1,
    "react/jsx-indent":[1, 4],
    "react/prop-types": 0,
    "react/prefer-stateless-function":0,
    "one-var":0,
    "prefer-template": 0,
    "import/no-unresolved": 0
  }
}
